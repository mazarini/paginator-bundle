{% if pages.isPagesDisplay %}
    <nav aria-label="Page navigation">
        <ul class="pagination justify-content-center">
            {% for page in pages %}
                <li class="page-item{{ page_class(page) }}">
                    <a class="page-link" href="{% block href %}{% endblock %}">{{ page.label|raw }}</a>
                </li>
            {% endfor %}
        </ul>
    </nav>
{% endif %}
