{
    "name": "mazarini/paginator-bundle",
    "description": "Base tools for entities and pagination.",
    "type": "symfony-bundle",
    "minimum-stability": "dev",
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "docker": false
        },
        "public-dir": "tests/public"
    },
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "^6.3|^7.0",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^6.3|^7.0",
        "symfony/runtime": "^6.3|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "^6.3|^7.0",
        "symfony/dotenv": "^6.3|^7.0",
        "symfony/maker-bundle": "^1.50",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/yaml": "^6.3|^7.0"
    },
    "license": "GPL-3.0-or-later",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Mazarini\\PaginatorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mazarini\\PaginatorBundle\\Tests\\": "tests/tests",
            "App\\": "tests/src/"
        }
    },
    "replace": {},
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "authors": [
        {
            "name": "Mazarini",
            "email": "mazarini@protonmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    }
}
